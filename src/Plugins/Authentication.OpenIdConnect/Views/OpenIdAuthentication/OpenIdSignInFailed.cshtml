@{
    Layout = "_SingleColumn";
}
@model Authentication.OpenIdConnect.Models.FailedModel
<div class="page facebook-login">
    <div class="page-title">
        <h1>@Loc["Plugins.ExternalAuth.OIDC.Failed"]</h1>
    </div>
    <div class="page-body">
        <div class="alert alert-danger">
            @Loc["Plugins.ExternalAuth.OIDC.Failed.ErrorMessage"]: @Model.ErrorMessage
        </div>
    </div>
</div>
